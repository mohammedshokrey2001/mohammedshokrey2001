year = str(int(input())+1)
mar = True
while mar :
 if year[0]!=year[1] and year[0]!= year[2] and year[0] != year[3] \
         and year[1]!=year[0] and year[1]!= year[2] and year[1] != year[3] and\
         year[2]!=year[1] and year[2]!= year[0] and year[0] != year[3] and year[3]!=year[1]\
         and year[3]!= year[2] and year[3] != year[0]:
                   print(year)
                   mar = False
 
 else :
        year =str(int(year)+1)
