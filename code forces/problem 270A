

def main():
    lis_angle = []
    for i in range (3,1000):
        y = (i-2)*180
        y = y/i
        lis_angle.append(y)

    n = int(input())
    for i in range(0,n):
        y = int(input())
        if y in lis_angle:
            print("YES")
        else:
            print("NO")


if __name__ == '__main__':
    main()
